# 🌐 ScholarBEE Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# 🔗 API Configuration
VITE_API_URL=http://localhost:3000/api
# For production, use: https://your-backend-domain.com/api

# 🚀 App Configuration
VITE_APP_NAME=ScholarBEE
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Empowering students through scholarships

# 📧 Contact Configuration
VITE_CONTACT_EMAIL=contact@scholarbee.com
VITE_SUPPORT_EMAIL=support@scholarbee.com

# 🔗 External Services
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_SENTRY_DSN=your-sentry-dsn

# 🌍 Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true 